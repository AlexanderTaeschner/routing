{
  "version": "1.0.0-*",

  "dependencies": {
    "NUnit": "3.4.1",
    "NetTopologySuite.IO.GeoJSON": "1.14.0",
    "NetTopologySuite": "1.14.0",
    "Itinero.Profiles": "1.0.0-*",
    "Itinero.IO.Osm": "1.0.0-*",
    "Itinero.Geo": "1.0.0-*",
    "Itinero": "1.0.0-*",
    "GeoAPI": "1.7.4",

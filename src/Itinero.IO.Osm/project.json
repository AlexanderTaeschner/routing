{
  "version": "1.0.0-*",
  "dependencies": {
    "Itinero": "1.0.0-*",
    "OsmSharp": "5.0.5-rc1"
  },
  "frameworks": {
    "netstandard1.6": {
      "buildOptions": {
        "define": [ "NETSTANDARD" ],
        "compile": {
          "includeFiles": [
            "../../SharedAssemblyVersion.cs"
          ]
        },
        "embed": {
          "include": [ "Profiles/Defaults/*.lua" ]
        }
      },
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Xml.XmlSerializer": "4.0.11"
      }
    },
    "net4.0": {
      "buildOptions": {
        "define": [ "NET40" ],
        "compile": {
          "includeFiles": [
            "../../SharedAssemblyVersion.cs"
          ]
        },
        "embed": {
          "include": [ "Profiles/Defaults/*.lua" ]
        }
      },
      "frameworkAssemblies": {
        "System.Xml": ""
      }
    },
    "net4.5": {
      "buildOptions": {
        "define": [ "NET45" ],
        "compile": {

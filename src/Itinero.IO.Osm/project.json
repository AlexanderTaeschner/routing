        "embed": {
          "include": [ "Profiles/Defaults/*.lua" ]
        }
      },
      "frameworkAssemblies": {
        "System.Xml": ""
      }
    },
    ".NETPortable,Version=v4.0,Profile=Profile147": {
      "buildOptions": {
        "define": [ "PORTABLE147" ],
        "compile": {
          "includeFiles": [
            "../../SharedAssemblyVersion.cs"
          ]
        },
        "embed": {
          "include": [ "Profiles/Defaults/*.lua" ]
        }
      },
      "frameworkAssemblies": {
        "System": "",
